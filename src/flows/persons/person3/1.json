{
    "header_text": "Person 3",
    "elements": [
        {
            "type": "Text",
            "text": "Name"
        },
        {
            "type": "entry",
            "placeholder": "Ex. John Doe",
            "variable_name": "global:person3name",
            "report": "Name",
            "Border": false
        },
        {
            "type": "Text",
            "text": "Relationship"
        },
        {
